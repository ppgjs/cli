  "license": "ISC",
  "devDependencies": {
    "@pz/cli": "link:",
    "@types/node": "^20.8.3",
    "eslint": "^8.51.0",
    "eslint-config-soybeanjs": "^0.5.6",
    "tsx": "^3.13.0",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0"
  },
  "dependencies": {
    "c12": "^1.4.2",
    "cac": "^6.7.14",
    "chalk": "^5.3.0",
